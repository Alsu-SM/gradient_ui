{
	"name": "gradient_ui",
	"private": true,
	"version": "0.2.0",
	"type": "module",
	"homepage": "https://alsu-sm.github.io/gradient_ui",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
