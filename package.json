		"eslint-plugin-simple-import-sort": "10.0.0",
		"gh-pages": "^5.0.0",
		"prettier": "2.8.8",
		"typescript": "5.1.6",
		"vite": "4.3.9"
	},
	"packageManager": "yarn@3.3.1"
